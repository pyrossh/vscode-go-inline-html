{
  "name": "vscode-go-inline-html",
  "displayName": "Inline HTML & CSS for go",
  "description": "Syntax highlighting for inline html and css in go",
  "version": "0.0.3",
  "license": "MIT",
  "publisher": "pyros2097",
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyros2097/vscode-go-inline-html.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/go-html.json",
        "scopeName": "html-string.injection",
        "injectTo": [
          "source.go"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.html": "html",
          "meta.embedded.block.css": "css"
        }
      }
    ]
  }
}
